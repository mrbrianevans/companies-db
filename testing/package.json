{
  "name": "api-testing",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "test": "node --experimental-specifier-resolution=node --loader ts-node/esm node_modules/mocha/lib/cli/cli.js tests\\personsWithSignificantControl.spec.ts --reporter=spec",
    "index": "node --loader ts-node/esm index.ts",
    "benchmark": "node --loader ts-node/esm benchmark.ts",
