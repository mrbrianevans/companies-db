    "mocha": "^10.0.0",
    "ts-node": "^10.7.0"
  },
  "devDependencies": {
    "@types/autocannon": "^7.6.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.34",
    "typescript": "^4.7.4"
  }
}
